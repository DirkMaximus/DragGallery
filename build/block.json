{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/drag-gallery",
  "version": "0.1.0",
  "title": "Drag Gallery",
  "category": "media",
  "icon": "layout",
  "description": "A Gallery block with Drag and Drop functionality to lay out images in rows while retaining aspect ratio.",
  "attributes": {
    "message": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "images": {
      "type": "array",
      "default": []
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "drag-gallery",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "script": "file:./draggallery.js"
}